<div class="row">
  <div class="col-sm-2"></div>
    <div class="col-sm-8">
      <h1>My Dogs</h1>

      <table>
        <thead>
          <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Breed</th>
            <th>Age</th>
          </tr>
        </thead>
        <tbody>
        <% @dogs.each do |dog| %>
          <tr>
            <td><%= dog.id %></td>
            <td><%= dog.name %></td>
            <td><%= dog.breed %></td>
            <td><%= dog.age %></td>
          </tr>
        <% end %>
      </tbody>

    </table>
  </div>
  <div class="col-sm-2"></div>
</div>